    <listOfRules>
      <assignmentRule metaid="metaid_0001135" variable="V_neg">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
        V_neg = -V</p>
          
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <ci> V </ci>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule metaid="metaid_0000135" variable="E">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
        E = V + E_R</p>
          
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <plus/>
            <ci> V </ci>
            <ci> E_R </ci>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule metaid="metaid_0000035" variable="V_L">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
        V_L = E_L - E_R</p>
          
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <ci> E_L </ci>
            <ci> E_R </ci>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule metaid="metaid_0000041" variable="beta_n">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
        beta_n = 0.125 * exp[V / 80.0]</p>
          
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <cn> 0.125 </cn>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <ci> V </ci>
                <cn> 80 </cn>
              </apply>
            </apply>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule metaid="metaid_0000038" variable="alpha_h">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
        alpha_h = 0.07 * exp[V / 20.0]</p>
          
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <cn> 0.07 </cn>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <ci> V </ci>
                <cn> 20 </cn>
              </apply>
            </apply>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule metaid="metaid_0000033" variable="V_Na">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
        V_Na = E_Na - E_R</p>
          
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <ci> E_Na </ci>
            <ci> E_R </ci>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule metaid="metaid_0000034" variable="V_K">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
        V_K = E_K - E_R</p>
          
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <ci> E_K </ci>
            <ci> E_R </ci>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule metaid="metaid_0000043" variable="i_K">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
        i_K = g_K * n^4.0 * (V - V_K)</p>
          
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:bqmodel="http://biomodels.net/model-qualifiers/" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/">
            <rdf:Description rdf:about="#metaid_0000043">
	<bqbiol:isVersionOf>
	<rdf:Bag>
	<rdf:li rdf:resource="http://identifiers.org/obo.go/GO:0006813"/>
	</rdf:Bag>
	</bqbiol:isVersionOf>
	</rdf:Description>
	
          </rdf:RDF>
        </annotation>
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <ci> g_K </ci>
            <apply>
              <power/>
              <ci> n </ci>
              <cn> 4 </cn>
            </apply>
            <apply>
              <minus/>
              <ci> V </ci>
              <ci> V_K </ci>
            </apply>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule metaid="metaid_0000042" variable="i_Na">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
        i_Na = g_Na * m^3.0 * h * (V - V_Na)</p>
          
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:bqmodel="http://biomodels.net/model-qualifiers/" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/">
            <rdf:Description rdf:about="#metaid_0000042">
	<bqbiol:isVersionOf>
	<rdf:Bag>
	<rdf:li rdf:resource="http://identifiers.org/obo.go/GO:0006814"/>
	</rdf:Bag>
	</bqbiol:isVersionOf>
	</rdf:Description>
	
          </rdf:RDF>
        </annotation>
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <ci> g_Na </ci>
            <apply>
              <power/>
              <ci> m </ci>
              <cn> 3 </cn>
            </apply>
            <ci> h </ci>
            <apply>
              <minus/>
              <ci> V </ci>
              <ci> V_Na </ci>
            </apply>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule metaid="metaid_0000044" variable="i_L">
        <notes>
          <p xmlns="http://www.w3.org/1999/xhtml">
        i_L = g_L * (V - V_L)</p>
          
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:bqmodel="http://biomodels.net/model-qualifiers/" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/">
            <rdf:Description rdf:about="#metaid_0000044">
	<bqbiol:isVersionOf>
