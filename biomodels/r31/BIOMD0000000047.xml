            <ci> p11 </ci>
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> p12 </ci>
                <ci> n </ci>
              </apply>
              <apply>
                <plus/>
                <ci> K1 </ci>
                <ci> n </ci>
              </apply>
            </apply>
