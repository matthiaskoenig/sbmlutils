      <assignmentRule metaid="metaid_0000023" variable="n">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <ci> n0 </ci>
            <apply>
              <plus/>
              <apply>
                <exp/>
                <apply>
                  <times/>
                  <apply>
                    <minus/>
                    <ci> kbN </ci>
                  </apply>
                  <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> Time </csymbol>
                </apply>
              </apply>
              <apply>
                <times/>
                <ci> kappa </ci>
                <apply>
                  <minus/>
                  <cn type="integer"> 1 </cn>
                  <apply>
                    <exp/>
                    <apply>
                      <times/>
                      <apply>
                        <minus/>
                        <ci> kbN </ci>
                      </apply>
                      <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> Time </csymbol>
