          <times/>
          <apply>
            <minus/>
            <ci> Vmax_EX_Glcxt </ci>
          </apply>
          <ci> Glcxt </ci>
        </apply>
        <apply>
          <plus/>
          <ci> Km_EX_Glcxt </ci>
          <ci> Glcxt </ci>
        </apply>
      </apply>
    </math>
  </assignmentRule>
  <assignmentRule variable="lb_kin_EX_O2">
    <math xmlns="http://www.w3.org/1998/Math/MathML">
