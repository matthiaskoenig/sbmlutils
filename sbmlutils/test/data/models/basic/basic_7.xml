      <speciesReference species="e__gal" stoichiometry="1" constant="true"/>
    </listOfReactants>
    <listOfProducts>
      <speciesReference species="c__gal" stoichiometry="1" constant="true"/>
    </listOfProducts>
    <kineticLaw>
      <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
        <apply>
          <divide/>
          <apply>
            <times/>
            <apply>
              <divide/>
              <ci> GLUT2_Vmax </ci>
              <ci> GLUT2_k_gal </ci>
            </apply>
            <apply>
              <minus/>
              <ci> e__gal </ci>
              <apply>
                <divide/>
                <ci> c__gal </ci>
                <ci> GLUT2_keq </ci>
              </apply>
            </apply>
          </apply>
          <apply>
            <plus/>
            <cn sbml:units="dimensionless" type="integer"> 1 </cn>
            <apply>
              <divide/>
              <ci> c__gal </ci>
              <ci> GLUT2_k_gal </ci>
            </apply>
            <apply>
              <divide/>
              <ci> e__gal </ci>
              <ci> GLUT2_k_gal </ci>
            </apply>
          </apply>
        </apply>
      </math>
    </kineticLaw>
  </reaction>
