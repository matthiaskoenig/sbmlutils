    <listOfUnitDefinitions>
      <unitDefinition id="hr" name="hr">
        <listOfUnits>
          <unit kind="second" exponent="1" scale="0" multiplier="3600"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="m2" name="meter^2">
