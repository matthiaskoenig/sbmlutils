    <annotation>
      <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
        <rdf:Description rdf:about="#meta_parameter">
          <dcterms:creator>
            <rdf:Bag>
              <rdf:li rdf:parseType="Resource">
                <vCard:N rdf:parseType="Resource">
                  <vCard:Family>König</vCard:Family>
                  <vCard:Given>Matthias</vCard:Given>
                </vCard:N>
                <vCard:EMAIL>koenigmx@hu-berlin.de</vCard:EMAIL>
                <vCard:ORG rdf:parseType="Resource">
                  <vCard:Orgname>Humboldt-University Berlin, Institute for Theoretical Biology</vCard:Orgname>
                </vCard:ORG>
              </rdf:li>
            </rdf:Bag>
          </dcterms:creator>
          <dcterms:created rdf:parseType="Resource">
            <dcterms:W3CDTF>1900-01-01T00:00:00Z</dcterms:W3CDTF>
          </dcterms:created>
          <dcterms:modified rdf:parseType="Resource">
            <dcterms:W3CDTF>1900-01-01T00:00:00Z</dcterms:W3CDTF>
          </dcterms:modified>
        </rdf:Description>
      </rdf:RDF>
    </annotation>
    <listOfUnitDefinitions>
      <unitDefinition id="hr" name="hr">
        <listOfUnits>
          <unit kind="second" exponent="1" scale="0" multiplier="3600"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="kg" name="kg">
        <listOfUnits>
          <unit kind="gram" exponent="1" scale="0" multiplier="1000"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="l_per_hr" name="liter/hr">
        <listOfUnits>
          <unit kind="litre" exponent="1" scale="0" multiplier="1"/>
          <unit kind="second" exponent="-1" scale="0" multiplier="3600"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="l_per_kg" name="liter/kg">
        <listOfUnits>
          <unit kind="litre" exponent="1" scale="0" multiplier="1"/>
          <unit kind="gram" exponent="-1" scale="0" multiplier="1000"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="m2" name="meter^2">
        <listOfUnits>
          <unit kind="metre" exponent="2" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mg" name="mg">
        <listOfUnits>
          <unit kind="gram" exponent="1" scale="0" multiplier="0.001"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mg_per_hr" name="mg/hr">
        <listOfUnits>
          <unit kind="gram" exponent="1" scale="0" multiplier="0.001"/>
          <unit kind="second" exponent="-1" scale="0" multiplier="3600"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mg_per_l" name="mg/liter">
        <listOfUnits>
          <unit kind="gram" exponent="1" scale="0" multiplier="0.001"/>
          <unit kind="litre" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfParameters>
      <parameter id="p1" value="1" units="mg" constant="true">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>Constant parameter with value.</p>
          </body>
        </notes>
      </parameter>
      <parameter id="p2" value="2e-05" units="mg" constant="true">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>Constant parameter with value in E notation.</p>
          </body>
        </notes>
      </parameter>
      <parameter id="p3" value="NaN" units="mg" constant="true">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>Constant parameter with value set via InitialAssignment.</p>
          </body>
        </notes>
      </parameter>
      <parameter id="p4" value="NaN" units="mg" constant="false">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>Parameter set via time-dependent AssignmentRule.</p>
          </body>
        </notes>
      </parameter>
      <parameter metaid="meta_dose" sboTerm="SBO:0000002" id="dose" value="10" units="mg" constant="true">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>Parameter with SBOTerm and annotations.</p>
          </body>
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_dose">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000002"/>
                  <rdf:li rdf:resource="https://identifiers.org/ncit/C25488"/>
                  <rdf:li rdf:resource="https://identifiers.org/efo/0000428"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
      </parameter>
      <parameter id="p5" value="NaN" units="mg" constant="false"/>
    </listOfParameters>
    <listOfInitialAssignments>
      <initialAssignment symbol="p3">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>Sets the initial value of p3 as the sum of p1 and p2.</p>
          </body>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <plus/>
            <ci> p1 </ci>
            <ci> p2 </ci>
          </apply>
        </math>
      </initialAssignment>
    </listOfInitialAssignments>
    <listOfRules>
      <assignmentRule variable="p4">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>Sets p4 as a time dependent parameter via an assignment rule.</p>
          </body>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
          <apply>
            <plus/>
            <cn sbml:units="mg" type="integer"> 10 </cn>
            <apply>
              <times/>
              <cn sbml:units="mg"> 2 </cn>
              <apply>
                <sin/>
                <apply>
                  <divide/>
                  <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> time </csymbol>
                  <cn sbml:units="hr" type="integer"> 1 </cn>
                </apply>
              </apply>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="p5">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
            <p>Creates parameter p5 and sets it via a time dependent assignment rule.</p>
          </body>
        </notes>
        <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
          <apply>
            <plus/>
            <cn sbml:units="mg" type="integer"> 10 </cn>
            <apply>
              <times/>
              <cn sbml:units="mg"> 2 </cn>
              <apply>
                <sin/>
                <apply>
                  <divide/>
                  <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> time </csymbol>
                  <cn sbml:units="hr" type="integer"> 1 </cn>
                </apply>
              </apply>
            </apply>
          </apply>
        </math>
      </assignmentRule>
    </listOfRules>
  </model>
</sbml>
