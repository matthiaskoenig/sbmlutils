          <apply>
            <plus/>
            <apply>
              <times/>
              <cn type="integer"> -1 </cn>
              <ci> k2 </ci>
            </apply>
            <ci> S1 </ci>
            <ci> S2 </ci>
          </apply>
        </math>
      </algebraicRule>
    </listOfRules>
  </model>
